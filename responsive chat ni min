def get_response(user_input):
    if "hello" in user_input: return "HELLOOOOOOO POOOOOOO!!"
    if "sino ako" in user_input: return "mamaw mag programming dba?"
    if "bye" in user_input: return "iwan ako tangina"
    return "di kita maintindihan gago ka"
def chat():
    print("welcome kapatid, Type 'bye' to exit.")
    while True:
        user_input = input("You: ")
        response = get_response(user_input.lower())
        print(f"AI: {response}")
        if "bye" in user_input: break
if __name__ == "__main__": chat()